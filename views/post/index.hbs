<main>
  <section class="blog-main">
    <img src="{{post.images.image}}" alt="blog page">
    <div class="blog-main_info">
    <h1>{{post.head}}</h1>
      <h3>{{post.subtitle}}</h3>
      <span class="blog-date">{{post.createdAt}}</span>
      <p class="blog-timeToRead">read in {{post.minutes_to_read}} mins
    </div>
  </section>
  <section class="blog-content">
    {{{post.body}}}
  </section>
  <section class="blog-footer">
    <div class="blog-socials">
      <p>Share</p>
      <div class="blog-social-list">
        <a href="https://www.facebook.com/sharer/sharer.php?u={{API_URL}}" class="icon-wrapper">
