    </div>
    <div class="blog-arrows-wrapper">
     {{#if previousPost}}
      <a href="{{ previousPost.url }}">
        <div class="blog-preview">
          <img class="blog-preview-img" src="{{previousPost.images.previewImage}}" alt="preview"/>
          <div class="blog-preview-content">
            <h2 class="blog-preview-title">{{previousPost.head}}</h2>
            <p class="blog-preview-description">{{previousPost.excerpt}}</p>
            <span class="blog-preview-link">Read more</span>
          </div>
        </div>
      </a>
     {{/if}}
    {{#if nextPost}}
      <a href="{{ nextPost.url }}">
        <div class="blog-preview">
          <img class="blog-preview-img" src="{{nextPost.images.previewImage}}" alt="preview"/>
          <div class="blog-preview-content">
            <h2 class="blog-preview-title">{{nextPost.head}}</h2>
            <span class="blog-preview-description">{{nextPost.excerpt}}</span>
            <span class="blog-preview-link">Read more</span>
          </div>
        </div>
      </a>
      {{/if}}
    </div>
  </section>
</main>

